# Tour
$GEM5/build/ARM/gem5.opt --outdir="sims/tour_16" assignment2.py --prog="queens" --bp="TournamentBP" --bp_size="16" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/tour_32" assignment2.py --prog="queens" --bp="TournamentBP" --bp_size="32" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/tour_64" assignment2.py --prog="queens" --bp="TournamentBP" --bp_size="64" --bp_bits="2"
