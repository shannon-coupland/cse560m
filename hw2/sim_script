$GEM5/build/ARM/gem5.opt --outdir="sims/loc2_512" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="512" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc2_1024" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="1024" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc2_2048" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="2048" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc2_4096" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="4096" --bp_bits="2"
