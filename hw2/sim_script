$GEM5/build/ARM/gem5.opt --outdir="sims/tour_256" assignment2.py --prog="queens" --bp="TournamentBP" --bp_size="256" --bp_bits="2"
$GEM5/build/ARM/gem5.opt --outdir="sims/tour_512" assignment2.py --prog="queens" --bp="TournamentBP" --bp_size="512" --bp_bits="2"
