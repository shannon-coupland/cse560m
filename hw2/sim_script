$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_256" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="256" --bp_bits="1"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_512" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="512" --bp_bits="1"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_1024" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="1024" --bp_bits="1"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_2048" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="2048" --bp_bits="1"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_4096" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="4096" --bp_bits="1"
$GEM5/build/ARM/gem5.opt --outdir="sims/loc1_8192" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="8192" --bp_bits="1"


# Loc2
$GEM5/build/ARM/gem5.opt --outdir="sims/loc2_16" assignment2.py --prog="queens" --bp="LocalBP" --bp_size="16" --bp_bits="2"
